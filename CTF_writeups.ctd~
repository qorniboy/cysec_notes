			<rich_text>interesting file, script.js in port 80

(function(u, R) {
	function W(u, R, k, U, r) {
		return i(r - 0xa9, u);
	}

	function L(u, R, k, U, r) {
		return i(R - 0x32c, U);
	}

	function p(u, R, k, U, r) {
		return i(k - -0x15, U);
	}

	function O(u, R, k, U, r) {
		return i(R - -0x298, k);
	}
	var k = u();

	function z(u, R, k, U, r) {
		return i(k - -0x75, r);
	}
